import com.rameses.annotations.*;
import com.rameses.common.*;
import java.rmi.server.UID
import com.rameses.services.extended.*;

class ProvinceContextService extends ActiveCrudService {
	
	@ActiveDB('province')	
	def em
	
	@Service("OrgService")
	def org;

	public void beforeOpen(o) {
		o.objid = org.getRoot().objid;
	}

	public void afterOpen(o) {
		def m = org.open(o);
		o.putAll( m );
	}


	public void beforeUpdate(o) {
		org.update(o);
	}	

	public void beforeRemoveEntity(o) {
		throw new Exception("Province cannot be removed");	
	}	
	

}
